<body>
<div>
    <h1>@ViewBag.title</h1>
</div>

 <table class="customstripe">
    @foreach(var job in ViewBag.Jobs)
    {
        <tr>
             <td>Id</td>
             <td>@job.Id</td>
        </tr>
        <tr>
             <td>Name</td>
             <td>@job.Name</td>
        </tr>
        <tr>
             <td>Employer</td>
             <td>@job.Employer</td>
        </tr>
        <tr>
             <td>Location</td>
             <td>@job.Location</td>
        </tr>
        <tr>
             <td>Position Type</td>
             <td>@job.PositionType</td>
        </tr>
        <tr>
             <td>Skill</td>
             <td>@job.CoreCompetency</td>
        </tr>
    }
</table>
</body>

   
